<?xml version="1.0" encoding="UTF-8"?>


<assembly>
    <id>jar-with-dependencies</id>
    <formats>
        <format>jar</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <dependencySets>
        <dependencySet>
            <outputDirectory>modules</outputDirectory>
            <includes>
                <include>eu.essi_lab:vlab*</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory>libs</outputDirectory>
            <useProjectArtifact>true</useProjectArtifact>
            <scope>runtime</scope>
            <excludes>
                <exclude>eu.essi_lab:vlab*</exclude>
            </excludes>
        </dependencySet>
    </dependencySets>
</assembly>