<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://www.oracle.com/webfolder/technetwork/jsc/xml/ns/javaee/web-app_3_0.xsd">

    <display-name>${project.name} ${project.version}</display-name>

    <servlet>
        <servlet-name>eu.essi_lab.vlab.web.api.VLabApplication</servlet-name>
    </servlet>

    <servlet-mapping>
        <servlet-name>eu.essi_lab.vlab.web.api.VLabApplication</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>


    <listener>
        <listener-class>eu.essi_lab.vlab.web.api.servlet.BPServletListener</listener-class>
    </listener>


</web-app>
